<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"></setting>
		<!-- 만약에 null로 데이터가 전달되었으면 빈칸이 아닌 "NULL"로 인식하도록 설정 -->
	</settings>
	<typeAliases>
		<typeAlias type="com.kh.spouting.user.domain.User" alias="User"/>
		<typeAlias type="com.kh.spouting.center.domain.Center" alias="Center"/>
		<typeAlias type="com.kh.spouting.book.domain.Facilities" alias="Facilities"/>
		<typeAlias type="com.kh.spouting.meeting.domain.Meeting" alias="Meeting"/>
		<typeAlias type="com.kh.spouting.notice.domain.Notice" alias="Notice"/>
		<typeAlias type="com.kh.spouting.product.domain.Product" alias="Product"/>
		<typeAlias type="com.kh.spouting.product.domain.Category" alias="Category"/>
		<typeAlias type="com.kh.spouting.diary.domain.Diary" alias="Diary"/>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/user-mapper.xml"/>
		<mapper resource="mappers/center-mapper.xml"/>
		<mapper resource="mappers/facilities-mapper.xml"/>
		<mapper resource="mappers/meeting-mapper.xml"/>
		<mapper resource="mappers/notice-mapper.xml"/>
		<mapper resource="mappers/product-mapper.xml"/>
		<mapper resource="mappers/diary-mapper.xml"/>
	</mappers>
</configuration>